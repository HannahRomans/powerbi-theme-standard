{
	"name": "Roman's Pizza Theme",
	"dataColors": ["#027DC8", "#E5171F", "#1FD5F1", "#FCFCFC", "#0C4480", "#66C266", "#F28C28", "#FFD700", "#7D3CCF"],
	"visualStyles": {
		"tableEx": {
			"*": {
				"grid": [{
					"gridVertical": false,
					"gridHorizontal": true,
					"gridHorizontalColor": { "solid": { "color": "#E5E5E5"}},
					"gridHorizontalWeight": 1,
					"rowPadding": 0,
					"outlineColor": { "solid": { "color": "#C8C6C4"}},
					"outlineWeight": 2,
					"textSize": 10
				}],
				"columnHeaders": [{
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#062240"}},
					"autoSizeColumnWidth": true,
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"alignment": "Left",
					"wordWrap": true
				}],
				"values": [{
					"fontColorPrimary": { "solid": { "color": "#FFFFFF"}},
					"backColorPrimary": { "solid": { "color": "#062240"}},
					"fontColorSecondary": { "solid": { "color": "#FFFFFF"}},
					"backColorSecondary": { "solid": { "color": "#062240"}},
					"wordWrap": true,
					"fontFamily": "Segoe UI",
					"fontSize": 10
				}],
				"total": [{
					"totals": true,
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#062240"}},
					"outline": "Frame",
					"fontFamily": "Segoe UI",
					"fontSize": 10
				}],
				"stylePreset": [{ 
					"name": "BoldHeader"
				}]
			}
		},
		"textbox": {
			"*": {
				"*": [{
					"fontSize": 28,
					"fontFamily": "Segoe (Bold)",
					"wordWrap": true,
					"color": { "solid": { "color": "#FFFFFF"} }
				}],
				"border": [{
					"color": { "solid": { "color": "#000000"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#FFFFFF" } },
					"background": { "solid": { "color": "#013F64" } },
					"alignment": "center"
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#013F64"}},
					"transparency": 0
				}]
			}
		},
		"pivotTable": {
			"*": {
				"stylePreset":[{
                    			"name":"Condensed"
                			}],
					"grid": [{
					"gridVertical": false,
					"gridHorizontal": true,
					"gridHorizontalColor": { "solid": { "color": "#BDBDBD"}},
					"gridHorizontalWeight": 1,
					"rowPadding": 0
				}],
				"columnHeaders": [{
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#013F64"}},
					"autoSizeColumnWidth": true,
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"alignment": "Left",
					"wordWrap": true
				}],
				"rowHeaders": [{
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#062240"}},
					"autoSizeColumnWidth": true,
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"alignment": "Left",
					"wordWrap": true
				}],
				"values": [{
					"fontColorPrimary": { "solid": { "color": "#FFFFFF"}},
					"backColorPrimary": { "solid": { "color": "#062240"}},
					"fontColorSecondary": { "solid": { "color": "#FFFFFF"}},
					"backColorSecondary": { "solid": { "color": "#062240"}},
					"wordWrap": true,
					"fontFamily": "Segoe UI",
					"fontSize": 10
				}],
				"subTotals": [{
					"rowSubtotals": true,
					"columnSubtotals": false,
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"fontFamily": "Segoe UI",
					"backColor": { "solid": { "color": "#013F64"}},
					"fontSize": 10,
					"applyToHeaders": true,
					"rowSubtotalsPosition": "Bottom",
					"perRowLevel": true,
					"perColumnLevel": true					
				}],
				"total": [{
					"totals": true,
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#062240"}},
					"outline": "Frame",
					"fontFamily": "Segoe UI",
					"fontSize": 10
				}]
			}
		},
		"multiRowCard": {
			"*": {
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"dataLabels": [{
					"color": { "solid": { "color": "#027DC8"}},
					"fontSize": 10,
					"fontFamily": "Segoe UI"
				}],
				"categoryLabels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"fontSize": 10,
					"fontFamily": "Segoe UI"
				}]
			}
		},
		"*": {
			"*": {
				"values": [{
					"fontColor": { "solid": { "color": "#FFFFFF" }},
					"fontFamily": "Segoe UI",
					"fontSize": 10
				}],
				"categoryAxis": [{
					"show": true,
					"labelColor": { "solid": { "color": "#FFFFFF" }},
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"titleColor": { "solid": { "color": "#FFFFFF" }},
					"titleFontFamily": "Segoe UI",
					"titleFontSize": 14
					
				}],
				"valueAxis": [{
					"show": true,
					"labelColor": { "solid": { "color": "#FFFFFF" }},
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"titleColor": { "solid": { "color": "#FFFFFF" }},
					"titleFontFamily": "Segoe UI",
					"titleFontSize": 14,
					"gridlineShow": true,
					"gridlineColor": { "solid": { "color": "#FFFFFF"}},
					"gridlineThickness": 1,
					"gridlineStyle": "dashed",
					"gridlineTransparency": 50
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"labelColor": { "solid": { "color": "#FFFFFF"}},
					"fontFamily": "Segoe UI",
					"fontSize": 10,
					"defaultToCircle": true
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center",
					"fontSize": 16,
					"fontFamily": "Segoe UI Semibold"
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#062240" } },
					"transparency": 0
				}],
				"border": [{
					"show": true,
					"color": { "solid": { "color": "#000000" } },
					"radius": 10
				}]
			}
		},
		"page": {
			"*": {
				"background": [{
					"color": { "solid": { "color": "#000000" } },
					"image": {
						"name": "background",
						"scaling": "Fit",
						"url": "<Base64 Encode>"
					},
					"transparency": 0
				}]
			}
		}
	}
}
